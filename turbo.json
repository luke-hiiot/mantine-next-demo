{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": [".next/**"],
      "dependsOn": ["^build"],
      "cache": true
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": ["tsconfig.tsbuildinfo?"]
    },
    "test": {
      "outputs": ["coverage/**"]
    }
  }
}
